<p id="notice"><%= notice %></p>

<h1>Listing Feedings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Date</th>
      <th>Time</th>
      <th>Place</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Price</th>
      <th>Feeding pics</th>
      <th>Pax limit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedings.each do |feeding| %>
      <tr>
        <td><%= feeding.user_id %></td>
        <td><%= feeding.date %></td>
        <td><%= feeding.time %></td>
        <td><%= feeding.place %></td>
        <td><%= feeding.latitude %></td>
        <td><%= feeding.longitude %></td>
        <td><%= feeding.price %></td>
        <td><%= feeding.feeding_pics %></td>
        <td><%= feeding.pax_limit %></td>
        <td><%= link_to 'Show', feeding %></td>
        <td><%= link_to 'Edit', edit_feeding_path(feeding) %></td>
        <td><%= link_to 'Destroy', feeding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feeding', new_feeding_path %>
